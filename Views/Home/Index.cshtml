
<div id="tabs" class="container-fluid">
    <h2>Dodajanje</h2>
    <div>
        <ul class="nav nav-tabs" id="myTabs" role="tablist">
            <li class="nav-item" role="presentation">
                <a class="nav-link active" id="slike-tab" data-bs-toggle="tab" href="#slike" role="tab" aria-controls="slike" aria-selected="true">Slike</a>
            </li>
            <li class="nav-item" role="presentation">
                <a class="nav-link" id="uporabniki-tab" data-bs-toggle="tab" href="#uporabniki" role="tab" aria-controls="uporabniki" aria-selected="false">Uporabniki</a>
            </li>
            <li class="nav-item" role="presentation">
                <a class="nav-link" id="racuni-tab" data-bs-toggle="tab" href="#racuni" role="tab" aria-controls="racuni" aria-selected="false">Racuni</a>
            </li>
        </ul>
        <div class="tab-content">
            <div class="tab-pane fade show active" id="slike" role="tabpanel" aria-labelledby="slike-tab">
                @Html.Partial("_Slike")
            </div>
            <div class="tab-pane fade" id="uporabniki" role="tabpanel" aria-labelledby="uporabniki-tab">
                @Html.Partial("_Uporabniki")
            </div>
            <div class="tab-pane fade" id="racuni" role="tabpanel" aria-labelledby="racuni-tab">
                @Html.Partial("_Racuni")
            </div>
        </div>
    </div>
</div>